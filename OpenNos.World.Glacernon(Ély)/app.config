<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
		<section name="eventScheduler" type="OpenNos.GameObject.EventSchedule, OpenNos.GameObject" />

		<section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
	</configSections>
	<connectionStrings>
		<add name="OpenNosContext" providerName="System.Data.SqlClient" connectionString="Data Source=localhost;Initial Catalog=NosmonsterV3;Integrated Security=true" />
	</connectionStrings>
	<entityFramework>
		<defaultConnectionFactory type="System.Data.Entity.Infrastructure.LocalDbConnectionFactory, EntityFramework">
			<parameters>
				<parameter value="mssqllocaldb" />
			</parameters>
		</defaultConnectionFactory>
		<providers>
			<provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
		</providers>
	</entityFramework>
	<log4net>
		<appender name="ManagedColoredConsoleAppender" type="log4net.Appender.ManagedColoredConsoleAppender">
			<mapping>
				<level value="FATAL" />
				<foreColor value="DarkRed" />
			</mapping>
			<mapping>
				<level value="ERROR" />
				<foreColor value="Red" />
			</mapping>
			<mapping>
				<level value="WARN" />
				<foreColor value="Yellow" />
			</mapping>
			<mapping>
				<level value="INFO" />
				<foreColor value="Green" />
			</mapping>
			<mapping>
				<level value="DEBUG" />
				<foreColor value="DarkMagenta" />
			</mapping>
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="[%level] %date{HH:mm:ss} -- %message%newline" />
			</layout>
		</appender>
		<appender name="FileAppender" type="log4net.Appender.FileAppender">
			<file value="Log.txt" />
			<appendToFile value="true" />
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline" />
			</layout>
		</appender>
		<root>
			<level value="ALL" />
			<appender-ref ref="ManagedColoredConsoleAppender" />
			<appender-ref ref="FileAppender" />
		</root>
	</log4net>
	<appSettings>
		<add key="LogPort" value="42069" />
		<add key="LogIP" value="82.165.183.105" />
		<add key="LogKey" value="LogKey" />
		<add key="MasterAuthKey" value="VHCbd76DHXd" />
		<add key="MasterPort" value="6969" />
		<add key="MasterIP" value="82.165.183.105" />
		<add key="SessionLimit" value="100" />
		<add key="Act4Port" value="5100" />
		<add key="Language" value="en" />
		<add key="WorldPort" value="5100" />
		<add key="MaxBankGold" value="100000000000" />
		<add key="AutoReboot" value="true" />
		<add key="IPAddress" value="82.165.183.105" />
		<add key="PublicIP" value="82.165.183.105" />
		<add key="ServerGroup" value="S1-NosmonsterV3" />
	<add key="DoubleExp" value="false" />
    <add key="DoubleFairyExp" value="false" />
    <add key="DoubleRaidBox" value="true" />
    <add key="DobleSpUp" value="false" />
    <add key="DobleEqUp" value="false" />
    <add key="dobleapuestaUpEv" value="false" />
    <add key="DoblePerfectionUpEv" value="false" />
    <add key="DobleExpeFamilyEv" value="false" />
    <add key="DobleGoldEv" value="true" />
    <add key="DobleReputEv" value="false" />
    <add key="DobleDropEv" value="false" />
    <add key="MultiEvent" value="true" />
	</appSettings>
	<eventScheduler>
		<schedule event="CALIGOR" time="00:30" />
		<schedule event="CALIGOR" time="02:30" />
		<schedule event="CALIGOR" time="04:30" />
		<schedule event="CALIGOR" time="06:30" />
		<schedule event="CALIGOR" time="08:30" />
		<schedule event="CALIGOR" time="10:30" />
		<schedule event="CALIGOR" time="12:30" />
		<schedule event="CALIGOR" time="14:30" />
		<schedule event="CALIGOR" time="16:30" />
		<schedule event="CALIGOR" time="18:30" />
		<schedule event="CALIGOR" time="20:30" />
		<schedule event="CALIGOR" time="22:30" />
		<schedule event="RANKINGREFRESH" time="03:00" />
		<schedule event="RANKINGREFRESH" time="06:00" />
		<schedule event="RANKINGREFRESH" time="09:00" />
		<schedule event="RANKINGREFRESH" time="12:00" />
		<schedule event="RANKINGREFRESH" time="15:00" />
		<schedule event="RANKINGREFRESH" time="18:00" />
		<schedule event="RANKINGREFRESH" time="21:00" />
		<schedule event="MINILANDREFRESHEVENT" time="00:00" />
	</eventScheduler>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
	</startup>
	<runtime>
		<gcServer enabled="true" />
		<assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
			<dependentAssembly>
				<assemblyIdentity name="System.Reactive" publicKeyToken="94bc3704cddfc263" culture="neutral" />
				<bindingRedirect oldVersion="0.0.0.0-4.2.0.0" newVersion="4.2.0.0" />
			</dependentAssembly>
			<dependentAssembly>
				<assemblyIdentity name="System.Threading.Tasks.Extensions" publicKeyToken="cc7b13ffcd2ddd51" culture="neutral" />
				<bindingRedirect oldVersion="0.0.0.0-4.2.1.0" newVersion="4.2.1.0" />
			</dependentAssembly>
			<dependentAssembly>
				<assemblyIdentity name="System.Runtime.CompilerServices.Unsafe" publicKeyToken="b03f5f7f11d50a3a" culture="neutral" />
				<bindingRedirect oldVersion="0.0.0.0-4.0.6.0" newVersion="4.0.6.0" />
			</dependentAssembly>
		</assemblyBinding>
	</runtime>
	<system.web>
		<membership defaultProvider="ClientAuthenticationMembershipProvider">
			<providers>
				<add name="ClientAuthenticationMembershipProvider" type="System.Web.ClientServices.Providers.ClientFormsAuthenticationMembershipProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" />
			</providers>
		</membership>
		<roleManager defaultProvider="ClientRoleProvider" enabled="true">
			<providers>
				<add name="ClientRoleProvider" type="System.Web.ClientServices.Providers.ClientRoleProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" cacheTimeout="86400" />
			</providers>
		</roleManager>
	</system.web>
</configuration>
